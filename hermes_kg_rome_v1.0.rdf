    <!-- http://www.istc.cnr.it/pstlab/ontologies/2023/1/hermes#ScalinataBorgiaDescription -->

    <owl:NamedIndividual rdf:about="http://www.istc.cnr.it/pstlab/ontologies/2023/1/hermes#ScalinataBorgiaDescription">
        <rdf:type rdf:resource="http://www.istc.cnr.it/pstlab/ontologies/2023/1/hermes#CulturalPropertyDescription"/>
