    <!-- http://www.istc.cnr.it/pstlab/ontologies/2023/1/hermes#ViaMadonnaMontiDescription -->

    <owl:NamedIndividual rdf:about="http://www.istc.cnr.it/pstlab/ontologies/2023/1/hermes#ViaMadonnaMontiDescription">
        <rdf:type rdf:resource="http://www.istc.cnr.it/pstlab/ontologies/2023/1/hermes#CulturalPropertyDescription"/>
        <l0:hasTopic rdf:resource="http://www.istc.cnr.it/pstlab/ontologies/2023/1/hermes#History_const"/>
        <l0:hasTopic rdf:resource="http://www.istc.cnr.it/pstlab/ontologies/2023/1/hermes#ProduzioneLibrariaAntica_const"/>
