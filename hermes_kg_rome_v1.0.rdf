    <!-- http://www.istc.cnr.it/pstlab/ontologies/2023/1/hermes#NagarKirtanDescription -->

    <owl:NamedIndividual rdf:about="http://www.istc.cnr.it/pstlab/ontologies/2023/1/hermes#NagarKirtanDescription">
        <rdf:type rdf:resource="http://www.istc.cnr.it/pstlab/ontologies/2023/1/hermes#CulturalPropertyDescription"/>
